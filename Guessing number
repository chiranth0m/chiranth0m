import random
sn=random.randint(1,20)
print('I have a number between 1 to 20 in my mind.')
for gn in range(1,7):
  print('Take a guess')
  guess=int(input())
  if guess<sn:
    print('No your number is lesser')
  elif guess>sn:
    print('No your number is greater')
  else:
    break
if guess==sn:
  print('Yes you guessed my number in '+str(gn)+' attempts')
else:
  print('No my number is '+str(sn))
